{
  "expo": {
    "name": "Sugarbum",
    "slug": "sugarbum",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191938"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sugarbum.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Sugarbum needs your location to show your partner where you are.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Sugarbum needs your location in the background to keep your partner updated even when the app is closed.",
        "NSLocationAlwaysUsageDescription": "Sugarbum needs your location in the background to keep your partner updated even when the app is closed.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.sugarbum.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#191938"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "0d649eda-35e1-40be-b52e-1b4b15048794"
      }
    }
  }
}